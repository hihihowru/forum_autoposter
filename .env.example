# ========================================
# 環境變數配置文件範例
# 請複製此文件為 .env 並填入真實的 API Key
# ========================================

# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here

# Google Sheets API
GOOGLE_CREDENTIALS_FILE=./credentials/google-service-account.json
GOOGLE_SHEETS_ID=your_google_sheets_id_here

# FinLab API
FINLAB_API_KEY=your_finlab_api_key_here

# CMoney API
CMONEY_BASE_URL=https://forumservice.cmoney.tw
CMONEY_LOGIN_URL=https://social.cmoney.tw/identity/token
CMONEY_PASSWORD=your_cmoney_password_here

# Serper API (for web search)
SERPER_API_KEY=your_serper_api_key_here

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/n8n_migration
REDIS_URL=redis://localhost:6379/0

# Application
DEBUG=True
LOG_LEVEL=INFO
SECRET_KEY=your_secret_key_here

# Task Queue
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# Docker Host API (for OHLC data)
DOCKER_HOST_API_URL=http://host.docker.internal:8000

# Alpha Vantage API (for stock data)
ALPHA_VANTAGE_API_KEY=demo_key

# CMoney 多帳號密碼
CMONEY_PASSWORD=your_cmoney_password_here
CMONEY_PASSWORD_201=your_cmoney_password_201_here
CMONEY_PASSWORD_202=your_cmoney_password_202_here
CMONEY_PASSWORD_203=your_cmoney_password_203_here
CMONEY_PASSWORD_204=your_cmoney_password_204_here
